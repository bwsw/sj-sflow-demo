{
  "name": "sflow-src-ip-output",
  "description": "Output module for sflow-demo",
  "input": "src-ip-stream",
  "output": "srcipdata",
  "checkpointMode": "every-nth",
  "checkpointInterval": 1,
  "parallelism": 1,
  "startFrom": "oldest",
  "perTaskCores": 0.1,
  "perTaskRam": 1024,
  "jvmOptions": {
    "-Xmx": "512m",
    "-XX:MaxMetaspaceSize=": "768m",
    "-XX:MaxDirectMemorySize=": "256m"
  },
  "coordinationService": "zookeeper-sflow-service"
}
