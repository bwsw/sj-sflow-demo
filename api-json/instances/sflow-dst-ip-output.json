{
  "name": "sflow-dst-ip-output",
  "description": "Output module for sflow example",
  "input": "dstipstream",
  "output": "dstipdata",
  "checkpointMode": "every-nth",
  "checkpointInterval": 3,
  "parallelism": 1,
  "startFrom": "oldest",
  "perTaskCores": 0.1,
  "perTaskRam": 192,
  "jvmOptions": {
    "-Xmx": "32m",
    "-Xms": "16m",
    "-XX:MaxDirectMemorySize=": "4m",
    "-XX:MaxMetaspaceSize=": "64m"
  },
  "coordinationService": "zookeeper-sflow-service"
}
