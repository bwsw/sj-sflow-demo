{
  "name": "sflow-process",
  "description": "Windowed module for sflow example",
  "window": "1",
  "slidingInterval": "1",
  "mainStream": "sflow",
  "batchFillType": {
    "typeName": "time-interval",
    "value": "300000"
  },
  "inputs": [
    "sflow-avro"
  ],
  "outputs": [
    "SrcAsStream",
    "DstAsStream",
    "SrcIpStream",
    "DstIpStream",
    "SrcDstStream"
  ],
  "stateManagement": "ram",
  "stateFullCheckpoint": 5,
  "parallelism": 1,
  "startFrom": "oldest",
  "perTaskCores": 0.1,
  "perTaskRam": 64,
  "eventWaitTime": 10,
  "coordinationService": "zookeeper-sflow-service"
}
