{
  "name" : "sflow-process",
  "description" : "Windowed module for sflow example",
  "window" : "1",
  "sliding-interval" : "1",
  "main-stream" : "sflow",
  "batch-fill-type" : {
	"type-name" : "time-interval",
	"value" : "300000"
   },
  "outputs" : ["SrcAsStream", "DstAsStream", "SrcIpStream", "DstIpStream","SrcDstStream"],
  "state-management" : "ram",
  "state-full-checkpoint" : 5,
  "parallelism" : 1,
  "start-from" : "oldest",
  "per-task-cores" : 0.1,
  "per-task-ram" : 64,
  "event-wait-time" : 10,
  "coordination-service" : "zookeeper-sflow-service"
}
