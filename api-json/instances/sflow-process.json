{
  "name": "sflow-process",
  "description": "Batch module for sflow example",
  "window": 8,
  "slidingInterval": 4,
  "inputs": [
    "sflow-avro"
  ],
  "outputs": [
    "src-ip-stream",
    "src-dst-stream"
  ],
  "stateManagement": "ram",
  "stateFullCheckpoint": 5,
  "parallelism": 1,
  "startFrom": "oldest",
  "perTaskCores": 0.1,
  "perTaskRam": 192,
  "jvmOptions": {
    "-Xmx": "64m",
    "-XX:MaxMetaspaceSize=": "120m",
    "-XX:MaxDirectMemorySize=": "4m"
  },
  "eventWaitTime": 1000,
  "coordinationService": "zookeeper-sflow-service",
  "options": {
    "ipv4-dat": "GeoIPASNum.dat"
  }
}
