{
  "name": "sflow-process",
  "description": "Windowed module for sflow example",
  "window": 1,
  "slidingInterval": 1,
  "inputs": [
    "sflow-avro"
  ],
  "outputs": [
    "srcasstream",
    "dstasstream",
    "srcipstream",
    "dstipstream",
    "srcdststream"
  ],
  "stateManagement": "ram",
  "stateFullCheckpoint": 5,
  "parallelism": 1,
  "startFrom": "oldest",
  "perTaskCores": 0.1,
  "perTaskRam": 64,
  "eventWaitTime": 10,
  "coordinationService": "zookeeper-sflow-service"
}
