{
  "name": "sflow-fallback-output",
  "description": "Output module for fallback",
  "input": "sflow-fallback",
  "output": "fallbackdata",
  "checkpointMode": "every-nth",
  "checkpointInterval": 1,
  "parallelism": 1,
  "startFrom": "oldest",
  "perTaskCores": 0.1,
  "perTaskRam": 192,
  "jvmOptions": {
    "-Xmx": "32m",
    "-Xms": "16m",
    "-XX:MaxDirectMemorySize=": "4m",
    "-XX:MaxMetaspaceSize=": "64m"
  },
  "coordinationService": "zookeeper-sflow-service",
  "inputAvroSchema": {
    "type": "record",
    "name": "fallback",
    "fields": [
      {
        "name": "data",
        "type": "string"
      }
    ]
  }
}
