{
  "name" : "sflow-input",
  "description" : "Input engine for sflow example",
  "outputs" : ["sflow"],
  "checkpoint-mode" : "every-nth",
  "checkpoint-interval" : 1000,
  "parallelism" : 1,
  "per-task-cores" : 0.1,
  "per-task-ram" : 64,
  "coordination-service" : "zookeeper-sflow-service",
  "lookup-history" : 100,
  "queue-max-size":100,
  "default-eviction-policy":"LRU",
  "eviction-policy":"expanded-time"
}
